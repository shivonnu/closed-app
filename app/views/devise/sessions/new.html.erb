<div class="contents row">
<div class="center">
  <div class="container">
    <h2>Log in</h2>
    <%= form_with model: @user, url: user_session_path, id: 'new_user', class: 'new_user', local: true do |f| %>
      <div class="field">
        <%= f.label :email %><br />
        <%= f.email_field :email, autofocus: true %>
      </div>

      <div class="field">
        <%= f.label :password %><br />
        <%= f.password_field :password, autocomplete: "off" %>
      </div>

            <div class = "formbrock">
              <div class="actions">
                <%= f.submit "Log in", class: "submit"%>
              </div>
            </div>
            <div class = "under-button">
            <div class = "right-header">
          <div class = "header-button">
            <%= link_to "新規登録画面へ", new_user_registration_path%>
          </div>
           </div>
        </div>
        </div>
     

    <% end %>
    </div>
  </div>
</div>
